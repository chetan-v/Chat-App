{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "YOUR_ENV_VARIABLE": "your_value"
    },
    "installCommand": "npm ci --ignore-scripts"
}
